{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "keyvalkanna"
		},
		"LS_BlobSA_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_BlobSA'"
		},
		"LS_KeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://nynika95.vault.azure.net/"
		},
		"LS_keuval_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sqlserverazure001.database.windows.net"
		},
		"LS_keuval_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "DBCloud"
		},
		"LS_keuval_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Ramya"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "keyvalkanna_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_BlobSA_connectionString": {
						"value": "[parameters('LS_BlobSA_connectionString')]"
					},
					"LS_KeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LS_keuval_properties_typeProperties_server": {
						"value": "[parameters('LS_keuval_properties_typeProperties_server')]"
					},
					"LS_keuval_properties_typeProperties_database": {
						"value": "[parameters('LS_keuval_properties_typeProperties_database')]"
					},
					"LS_keuval_properties_typeProperties_userName": {
						"value": "[parameters('LS_keuval_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}